<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"                    
                    xmlns:vmw="clr-namespace:TheseColorsDontRun.ViewModel.Workspaces;assembly=TheseColorsDontRun"
                    xmlns:vww="clr-namespace:TheseColorsDontRun.View.Workspaces;assembly=TheseColorsDontRun"
                    xmlns:viewmodel="clr-namespace:XRF_Data_Analysis_Utilities.ViewModel.Workspaces"
                    xmlns:view="clr-namespace:XRF_Data_Analysis_Utilities.View.Workspaces">

    <DataTemplate DataType="{x:Type vmw:ColorRampWorkspaceViewModel}">
        <vww:WideVerticalColorRampWorkspace />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:ImageDataWorkspaceViewModel}">
        <view:ImageDataWorkspace />
    </DataTemplate>

</ResourceDictionary>